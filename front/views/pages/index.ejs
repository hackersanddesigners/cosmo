<% const { title } = header %>

<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include( 'head' ) %>
    <title><%= title %></title>
  </head>
  <body>

    <%- include( 'header', header ) -%>

    <main>

      <h1> <%= title %> </h1>

      <section 
        class="articles"
      >
        <% for ( const article of articles ) { %> 
          <%- include( 'article-block', { article }) %> 
        <% } %> 
      </section>
    
    </main>

    <%- include( 'footer', footer ) -%>

  </body>
</html>
